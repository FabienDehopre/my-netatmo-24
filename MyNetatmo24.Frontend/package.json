{
  "name": "frontend-app",
  "version": "0.0.0",
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "dev": "run-script-os",
    "dev:win32": "vite --port %PORT%",
    "dev:default": "vite --port $PORT",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@angular/common": "catalog:angular",
    "@angular/compiler": "catalog:angular",
    "@angular/core": "catalog:angular",
    "@angular/forms": "catalog:angular",
    "@angular/platform-browser": "catalog:angular",
    "@angular/router": "catalog:angular",
    "@opentelemetry/auto-instrumentations-web": "catalog:opentelemetry",
    "@opentelemetry/context-zone": "catalog:opentelemetry",
    "@opentelemetry/exporter-trace-otlp-proto": "catalog:opentelemetry",
    "@opentelemetry/instrumentation": "catalog:opentelemetry",
    "@opentelemetry/resources": "catalog:opentelemetry",
    "@opentelemetry/sdk-trace-base": "catalog:opentelemetry",
    "@opentelemetry/sdk-trace-web": "catalog:opentelemetry",
    "@opentelemetry/semantic-conventions": "catalog:opentelemetry",
    "rxjs": "catalog:",
    "tslib": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "catalog:angular",
    "@angular/build": "catalog:angular",
    "@angular/cli": "catalog:angular",
    "@angular/compiler-cli": "catalog:angular",
    "@fabdeh/eslint-config": "catalog:eslint",
    "@tailwindcss/postcss": "catalog:tailwindcss",
    "@tailwindcss/vite": "catalog:tailwindcss",
    "@testing-library/angular": "catalog:testing-library",
    "@testing-library/jest-dom": "catalog:testing-library",
    "@testing-library/user-event": "catalog:testing-library",
    "@vitest/browser": "catalog:vitest",
    "@vitest/browser-playwright": "catalog:vitest",
    "@vitest/coverage-v8": "catalog:vitest",
    "@vitest/ui": "catalog:vitest",
    "daisyui": "catalog:tailwindcss",
    "eslint": "catalog:eslint",
    "eslint-plugin-better-tailwindcss": "catalog:eslint",
    "eslint-plugin-erasable-syntax-only": "catalog:eslint",
    "eslint-plugin-format": "catalog:eslint",
    "jsdom": "catalog:",
    "ng-packagr": "catalog:angular",
    "postcss": "catalog:tailwindcss",
    "run-script-os": "catalog:",
    "tailwindcss": "catalog:tailwindcss",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-node-polyfills": "catalog:",
    "vite-tsconfig-paths": "catalog:",
    "vitest": "catalog:vitest"
  }
}
